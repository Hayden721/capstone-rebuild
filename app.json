{
  "expo": {
    "name": "capstone-rebuild",
    "slug": "capstone-rebuild",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/connect-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo/connect-splash-android.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.capstonerebuild"
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "splash":{
        "image": "./assets/images/logo/connect-splash-android.png",
        "resizeMode":"cover",
        "backgroundColor":"#000000"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/connect-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.capstonerebuild"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photoPermission": "사진을 업로드하기 위해 앨범 접근 권한이 필요합니다."
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4d588621-5d8f-455d-8ffa-210537714c0f"
      }
    },
    "owner": "hayden1234"
  }
}
